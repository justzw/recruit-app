{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "jsx": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["../.eslintrc", "airbnb"],
  "rules": {
    "react/jsx-one-expression-per-line": [0],
    "comma-dangle": [0],
    "react/jsx-filename-extension": [0]
  }
}
